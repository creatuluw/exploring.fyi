############
# Supavisor -- Database pooler for Railway
############

POOLER_PROXY_PORT_TRANSACTION=6543
POOLER_DEFAULT_POOL_SIZE=20
POOLER_MAX_CLIENT_CONN=100
POOLER_TENANT_ID=railway-tenant-prod
POOLER_DB_POOL_SIZE=5

############
# API Proxy - Railway will provide external URLs
############

KONG_HTTP_PORT=8000
KONG_HTTPS_PORT=8443

############
# API - External URLs will be set by Railway
############

PGRST_DB_SCHEMAS=public,storage,graphql_public

############
# Auth - Railway production settings
############

SITE_URL=${RAILWAY_PUBLIC_URL:-http://localhost:3000}
ADDITIONAL_REDIRECT_URLS=
JWT_EXPIRY=3600
DISABLE_SIGNUP=false
API_EXTERNAL_URL=${RAILWAY_PUBLIC_URL:-http://localhost:8000}
SUPABASE_PUBLIC_URL=${RAILWAY_PUBLIC_URL:-http://localhost:8000}

############
# Mailer Config for Railway
############

MAILER_URLPATHS_CONFIRMATION="/auth/v1/verify"
MAILER_URLPATHS_INVITE="/auth/v1/verify"
MAILER_URLPATHS_RECOVERY="/auth/v1/verify"
MAILER_URLPATHS_EMAIL_CHANGE="/auth/v1/verify"

############
# Email auth - Configure with your SMTP provider
############

ENABLE_EMAIL_SIGNUP=true
ENABLE_EMAIL_AUTOCONFIRM=false
SMTP_ADMIN_EMAIL=admin@yourdomain.com
SMTP_HOST=smtp.yourmailprovider.com
SMTP_PORT=587
SMTP_USER=your-smtp-user
SMTP_PASS=your-smtp-password
SMTP_SENDER_NAME=Your App Name
ENABLE_ANONYMOUS_USERS=false

############
# Phone auth - Disable for Railway deployment
############

ENABLE_PHONE_SIGNUP=false
ENABLE_PHONE_AUTOCONFIRM=false

############
# Studio Dashboard
############

STUDIO_DEFAULT_ORGANIZATION=Railway Organization
STUDIO_DEFAULT_PROJECT=Railway Supabase

STUDIO_PORT=3000

############
# Functions
############

FUNCTIONS_VERIFY_JWT=false

############
# Logs - Railway Analytics
############

LOGFLARE_PUBLIC_ACCESS_TOKEN=railway-logflare-public-token-secure
LOGFLARE_PRIVATE_ACCESS_TOKEN=railway-logflare-private-token-secure

############
# Railway specific
############

DOCKER_SOCKET_LOCATION=/var/run/docker.sock

############
# Image processing
############

IMGPROXY_ENABLE_WEBP_DETECTION=true

############
# OpenAI API for SQL Assistant (optional)
############

OPENAI_API_KEY=

############
# Google Cloud (for analytics - optional)
############

GOOGLE_PROJECT_ID=
GOOGLE_PROJECT_NUMBER=
